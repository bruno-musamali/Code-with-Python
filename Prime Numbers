def is_prime(num):
    # Numbers less than or equal to 1 are not prime
    if num <= 1:
        return False
    # 2 and 3 are prime
    if num <= 3:
        return True
    # Check if divisible by 2 or 3
    if num % 2 == 0 or num % 3 == 0:
        return False
    # Check divisibility up to square root of num, stepping by 6
    i = 5
    while i * i <= num:
        if num % i == 0 or num % (i + 2) == 0:
            return False
        i += 6
    return True

# Example usage
number = int(input("Enter a number: "))
if is_prime(number):
    print(f"{number} is a prime number.")
else:
    print(f"{number} is not a prime number.")
